    <unit_specification>
        <unit_system_metric>
            <quantity_single>   <unit default="m"></unit> </quantity_single>
            <quantity_multiple>  <unit default="m"></unit> </quantity_multiple>
            <quantity_single_kilo>  <unit default="km"></unit> </quantity_single_kilo>
            <quantity_multiple_kilo>  <unit default="km"></unit> </quantity_multiple_kilo>
        </unit_system_metric>

        <unit_system_imperial>
            <quantity_single>   <unit default="ft"></unit> </quantity_single>
            <quantity_multiple>  <unit default="ft"></unit> </quantity_multiple>
            <quantity_single_kilo>  <unit default="mi"></unit> </quantity_single_kilo>
            <quantity_multiple_kilo>  <unit default="mi"></unit> </quantity_multiple_kilo>
        </unit_system_imperial>

        <unit_system_imperial_unused>
            <quantity_single>   <unit default="ft"></unit> </quantity_single>
            <quantity_multiple>  <unit default="ft"></unit> </quantity_multiple>
            <quantity_single_kilo>  <unit default="mi"></unit> </quantity_single_kilo>
            <quantity_multiple_kilo>  <unit default="mi"></unit> </quantity_multiple_kilo>
        </unit_system_imperial_unused>

    </unit_specification>

    <time_format_specification>
        <minutes>
            <fmt default="${minutes} mins"></fmt>
            <fmt regexpr="^1$">${minutes} min</fmt>
        </minutes>
        <minutes_short>
            <fmt default="${minutes}"></fmt>
            <fmt regexpr="^[0-9]$">0${minutes}</fmt>
        </minutes_short>
        <hours>
            <fmt default="${hours}:00 h"></fmt>
        </hours>
        <hours_short>
            <fmt default="${hours}"></fmt>
        </hours_short>
        <hours_and_minutes>
            <fmt default="${hours_short}:${minutes_short} h"></fmt>
        </hours_and_minutes>
    </time_format_specification>

